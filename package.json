{
  "name": "desktop-chat",
  "version": "0.0.1",
  "main": "electron/main.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cross-env BROWSER=none npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "vite build",
    "dist": "electron-builder",
    "agent:build": "tsc -p agent/tsconfig.json",
    "agent:run": "npm run agent:build && node agent/dist/examples/run.js"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "archiver": "^7.0.1",
    "better-sqlite3": "^9.4.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^35.7.5",
    "fast-glob": "^3.3.2",
    "pretty-bytes": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unzipper": "^0.11.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "electron-builder": "^24.6.4"
  }
}